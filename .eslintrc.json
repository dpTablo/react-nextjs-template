{
  "env": {
    "jest": true,
    "browser": true
  },
  "extends": [
    "next/core-web-vitals",
    "airbnb-typescript",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "indent": ["error", 4],

    "linebreak-style": ["error", "unix"],
    "function-paren-newline": ["error", "multiline"]
  }
}
